<query>
	<name>Cisco Device - Get CPUs</name>
	<description>Queries a Cisco device for linecard/module CPU Utilisation</description>
	<script_path>|path_cacti|/scripts/ss_cisco_cpu_usage.php</script_path>
	<script_function>ss_cisco_cpu_usage</script_function>
	<script_server>php</script_server>
	<arg_prepend>|host_hostname| |host_id| |host_snmp_version|:|host_snmp_port|:|host_snmp_timeout|:|host_ping_retries|:|host_max_oids|:|host_snmp_community|:|host_snmp_username|:|host_snmp_password|:|host_snmp_auth_protocol|:|host_snmp_priv_passphrase|:|host_snmp_priv_protocol|:|host_snmp_context|</arg_prepend>
	<arg_index>index</arg_index>
	<arg_num_indexes>num_indexes</arg_num_indexes>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimeter>:</output_delimeter>
	<index_order>cpuDesc</index_order>
	<index_order_type>alphabetic</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>

	<fields>
		<cpuDesc>
			<name>CPU Name</name>
			<direction>input</direction>
			<query_name>cpuDesc</query_name>
		</cpuDesc>
		<cpmCPUTotal1minRev>
			<name>1 Minute Average</name>
			<direction>output</direction>
			<query_name>cpmCPUTotal1minRev</query_name>
		</cpmCPUTotal1minRev>
		<cpmCPUTotal5minRev>
			<name>5 Minute Average</name>
			<direction>output</direction>
			<query_name>cpmCPUTotal1minRev</query_name>
		</cpmCPUTotal5minRev>
	</fields>
</query>
